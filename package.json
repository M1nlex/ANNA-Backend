{
  "name": "anna-backend",
  "version": "1.0.3",
  "description": "Backend for ANNA intranet",
  "main": "server.js",
  "author": "IpsaOne DevTeam",
  "private": true,
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "chance": "^1.0.13",
    "connect-redis": "^3.3.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-async-errors": "^2.1.1",
    "express-boom": "^2.0.0",
    "express-rate-limit": "^2.11.0",
    "express-request-id": "^1.4.0",
    "express-session": "^1.15.4",
    "find-root": "^1.1.0",
    "glob": "^7.1.2",
    "helmet": "^3.10.0",
    "joi": "^13.1.0",
    "marked": "^0.3.12",
    "mmmagic": "^0.4.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mv": "^2.1.1",
    "mysql2": "^1.5.1",
    "sequelize": "^4.32.2",
    "sequelize-fixtures": "^0.7.0",
    "sqlite3": "https://github.com/mapbox/node-sqlite3/tarball/master"
  },
  "devDependencies": {
    "autocannon": "^0.16.5",
    "ava": "^0.25.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "clinic": "^0.6.2",
    "eslint": "^4.16.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-jsdoc": "^3.3.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "mocha": "^4.1.0",
    "nsp": "^3.1.0",
    "sequelize-cli": "^3.2.0",
    "sequelize-mock": "^0.10.2",
    "sequelize-test-setup": "0.0.4",
    "sinon": "^4.2.2",
    "supertest": "^3.0.0",
    "umzug": "^2.1.0",
    "wrk": "^1.2.0"
  },
  "node_deb": {
    "entrypoints": {
      "daemon": "./app.js"
    }
  },
  "scripts": {
    "start": "node ./app.js",
    "test": "ava modules/**/test/*.js --verbose",
    "watch-test": "ava modules/**/test/*.js --watch --verbose",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "init-db": "npm run migrate && npm run seed",
    "clear-db": "sequelize db:migrate:undo:all",
    "lint": "eslint --cache --fix --max-warnings 50 .",
    "lint-nofix": "eslint --cache --max-warnings 50 .",
    "precommit": "cd ../../OneOs && vagrant ssh -c 'cd Anna/backend && bash pre-commit.sh'",
    "security": "nsp check .",
    "doc": "jsdoc ./ -r -c node_modules/jsdoc/conf.json",
    "debug": "node --inspect=0.0.0.0:9229 ./app.js"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
